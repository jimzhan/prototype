{
  "name": "prototype.js",
  "version": "0.1.0",
  "description": "Vue.js + Koa.js Application Boilerplate for Production",
  "main": "src/client/index.js",
  "scripts": {
    "//-0-": "====== Common shortcuts =======",
    "clean": "rimraf dist",
    "flow": "flow check",
    "lint": "eslint --ext=.js,.vue src test",
    "//-DevServer-": "====== Dev server with hotreload supports for Vue and API =======",
    "koaserver": "cross-env PORT=3001 babel-node src/server/index.js",
    "webpackserver": "webpack-dev-server --config etc/webpack.develop.js",
    "devserver": "npm run koaserver | npm run webpackserver",
    "//-Develop-": "====== [PM2] Dev server with hotreload supports for Vue and API =======",
    "prestart": "cross-env PORT=3001 pm2 start --watch --name server --interpreter babel-node src/server/index.js",
    "start": "pm2 start --name client webpack-dev-server -- --config etc/webpack.develop.js",
    "stop": "pm2 delete client",
    "poststop": "pm2 delete server",
    "//-Staging-": "======= Staging server with static assets supports =======",
    "prestart:staging": "webpack --config etc/webpack.staging.js",
    "start:staging": "pm2 start --name staging --interpreter babel-node src/server/index.js",
    "stop:staging": "pm2 delete staging",
    "poststop:staging": "npm run clean",
    "//-Release-": "======= Production release server =======",
    "prestart:release": "webpack --config etc/webpack.release.js",
    "start:release": "pm2 start --name release --interpreter babel-node src/server/index.js",
    "stop:release": "pm2 delete release",
    "poststop:release": "npm run clean",
    "//-X-": "======= Test case runner suites =======",
    "test": "mocha --require ignore-styles --compilers js:babel-core/register --recursive test/**/*.spec.js"
  },
  "repository": "https://github.com/jimzhan/vue.js",
  "author": "Jim Zhan <jim.zhan@me.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": ">= 7.10.0",
    "npm": ">= 4.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "axios": "^0.16.1",
    "chai": "^3.5.0",
    "dotenv": "^4.0.0",
    "glob": "^7.1.2",
    "koa": "^2.2.0",
    "koa-compose": "^4.0.0",
    "koa-router": "^7.2.0",
    "mocha": "^3.4.2",
    "pm2": "^2.4.6",
    "vue": "^2.3.3",
    "vue-router": "^2.5.3",
    "vuetify": "^0.12.1",
    "vuex": "^2.3.1",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-node6": "^11.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.3",
    "eslint-plugin-import": "^2.3.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.46.0",
    "html-webpack-plugin": "^2.28.0",
    "ignore-styles": "^5.0.1",
    "koa-static": "^3.0.0",
    "optimize-css-assets-webpack-plugin": "^1.3.2",
    "ora": "^1.2.0",
    "progress-bar-webpack-plugin": "^1.9.3",
    "rimraf": "^2.6.1",
    "url-loader": "^0.5.8",
    "vue-loader": "^12.1.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-merge": "^4.1.0"
  }
}
